#include<iostream>
using namespace std;
int main()
{
int a[50],b[50],c[100],i,k,n1,n2;
cout<<"enter the size of 1st array";
cin>>n1;
cout<<"enter the elements ";
for(i=0;i<n1;i++)
{
cin>>a[i];
c[i]=a[i];
}
k=i;
cout<<"enter the size of 2st array";
cin>>n2;
cout<<"enter the elements ";
for(i=0;i<n2;i++)
{
cin>>b[i];
c[k]=b[i];
k++;

}
cout<<"after merging";
for(i=0;i<k;i++)
{
	cout<<c[i]<<" ";
}


return 0;
}
